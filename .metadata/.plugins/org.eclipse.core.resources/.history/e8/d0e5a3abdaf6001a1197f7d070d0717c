	public Proceso(Properties prop) {
		this.prop=prop;
	}
	
	public void reemplazaCadena() {
		try {
			bfr=new BufferedReader(new FileReader(new File("Z:/Data/TPE/AAA/Reportes/20200914/cuentasBRM_AAA.sql")));
